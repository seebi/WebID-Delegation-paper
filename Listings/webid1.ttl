@prefix cert: <https://www.w3.org/ns/auth/cert#>.
@prefix xsd:  <https://www.w3.org/2001/XMLSchema#>.
@prefix foaf: <https://xmlns.com/foaf/0.1/>.
@prefix :   <https://montague.net/romeo>.

:romeo a foaf:Person;
  foaf:name "Romeo";
  cert:key [
    a cert:RSAPublicKey;
    cert:modulus "cb24...ed85"^^xsd:hexBinary;
    cert:exponent 65537;
   ];
  cert:secretary <https://montague.net/fb#laurence>.

